{
  "version": 2,
  "name": "escape-room-blackjack",
  "alias": [
    "blackjack.allthethings.win"
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {
      "src": "/assets/images/cards(.*)",
      "dest": "/assets/images/cards/$1",
      "headers": { "cache-control": "s-maxage=86400" }
    },
    {
      "src": "/assets/images/(.*)",
      "dest": "/assets/images/$1",
      "headers": { "cache-control": "s-maxage=86400" }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": { "cache-control": "s-maxage=86400" }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": { "cache-control": "s-maxage=86400" }
    }
  ]
}